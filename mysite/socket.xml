<!-- 若以socket方式访问 -->
<uwsgi>
    <socket>:80778</socket>
    <chdir>/home/Python/mysite</chdir>
    <module>mysite</module>
    <processes>4</processes> <!-- 进程数 --> 
    <daemonize>uwsgi.log</daemonize>
</uwsgi>

<!-- 若以http方式访问 -->
<uwsgi>
    <http>:8077</http>
    <chdir>/home/Python/mysite</chdir>
    <module>mysite</module>
    <processes>4</processes> <!-- 进程数 -->
    <daemonize>uwsgi.log</daemonize>
</uwsgi>
